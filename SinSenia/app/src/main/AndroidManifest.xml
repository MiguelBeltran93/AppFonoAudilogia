<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sinsenia.miguelbeltran.com.sinsenia">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_square_app"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SplashActivity" />
        <activity android:name=".RegisterUserActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".CreateSubjectActivity" />
        <activity android:name=".LessonsActivity" />
        <activity android:name=".ListSubjectActivity" />
        <activity android:name=".MenuActivity" />
        <activity android:name=".TeacherFeedbackActivity" />
        <activity android:name=".LessonTeacherActivity"></activity>
    </application>

</manifest>